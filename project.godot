; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Project Magus"
config/version="0.0.0"
run/main_scene="res://main_scenes/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")

[autoload]

InputContext="*res://contexts/input_context/input_context.tscn"
FocusContext="*res://contexts/focus_context/focus_context.tscn"
WorldContext="*res://contexts/world_context/world_context.tscn"
ItemContext="*res://contexts/item_context/item_context.tscn"
DndContext="*res://contexts/dnd_context/dnd_context.tscn"
TweenHelper="*res://utils/tween_helper/tween_helper.gd"
TooltipOverlay="*res://ui/tooltip_component/tooltip_overlay/tooltip_overlay.tscn"
PlayersContext="*res://contexts/players_context/players_context.tscn"
TimeContext="*res://contexts/time_context/TimeContext.tscn"
UiLayer="*res://ui/ui_layer/ui_layer.tscn"
OptionsContext="*res://contexts/options_context/options_context.tscn"
ControllerIcons="*res://addons/controller_icons/ControllerIcons.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=2
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/controller_icons/plugin.cfg", "res://addons/greenpixels-onscreen-keyboard/plugin.cfg")

[file_customization]

folder_colors={
"res://contexts/": "red",
"res://data_classes/": "orange",
"res://object_scenes/": "pink",
"res://resources/": "green",
"res://ui/": "gray"
}

[global_group]

tooltip_trigger=""
time_blocking="Nodes in this group will pause the game when they are inside the tree"
focusable=""
freeze_during_button_remap="Disables input processing while button remapping takes place"
popup=""

